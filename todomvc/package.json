{
    "name": "web-components-benchmark-todo",
    "version": "1.0.0",
    "description": "Web Components implementations benchmark todo benchmark",
    "scripts": {
        "benchmark": "node benchmarks/benchmark.js",
        "benchmark:tti": "node benchmarks/benchmark_load-metrics.js",
        "install:all": "./install-all.sh",
        "run:benchmark": "npm run benchmark",
        "build:angular": "cd angular-elements && npm run build",
        "build:atomico": "cd atomico && npm run build",
        "build:dojo": "cd dojo && npm run build",
        "build:hybrids": "cd hybrids && npm run build",
        "build:litelement": "cd lit-element && npm run build",
        "build:native": "cd native && npm run build",
        "build:hyperHTMLElement": "cd hyperHTMLElement && npm run build",
        "build:polymer3": "cd polymer-3 && npm run build",
        "build:riot": "cd riot && npm run build",
        "build:omi": "cd omi && npm run build",
        "build:skatejs:lithtml": "cd skatejs-lit-html && npm run build",
        "build:skatejs:preact": "cd skatejs-preact && npm run build",
        "build:slim": "cd slim.js && npm run build",
        "build:stencil": "cd stencil && npm run build",
        "build:svelte": "cd svelte && npm run build",
        "build:vue": "cd vue.js && npm run build",
        "build:all": "run-s build:angular build:atomico build:dojo build:hybrids build:litelement build:native build:hyperHTMLElement build:omi build:polymer3 build:riot build:skatejs:lithtml build:skatejs:preact build:slim build:stencil build:svelte build:vue"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vogloblinsky/web-components-benchmark.git"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/vogloblinsky/web-components-benchmark/issues"
    },
    "homepage": "https://github.com/vogloblinsky/web-components-benchmark#readme",
    "devDependencies": {
        "npm-run-all": "^4.1.5"
    }
}
